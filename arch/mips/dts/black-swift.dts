/dts-v1/;

#include "ar9331.dtsi"
#include <dt-bindings/gpio/gpio.h>

/ {
	model = "Black Swift";
	compatible = "smartlx,black-swift";

	memory {
		reg = <0x00000000 0x4000000>;
	};

	aliases {
		spiflash = &spiflash;
	};

	leds {
		compatible = "gpio-leds";

		s1 {
			label = "LED1";
			gpios = <&gpio 27 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
	};
};

&serial0 {
	status = "okay";
};

&gpio {
	status = "okay";
};

&spi {
	num-chipselects = <1>;
	status = "okay";

	/* Winbond W25Q128FV SPI flash */
	spiflash: m25p80@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "m25p80";
		spi-max-frequency = <104000000>;
		reg = <0>;
	};
};
