#include "skeleton.dtsi"
#include <arm/imx25.dtsi>

/ {
	aliases {
		mmc0 = &esdhc1;
		mmc2 = &esdhc2;
	};

	soc {
		iram: sram@78000000 {
			compatible = "mmio-sram";
			reg = <0x78000000 0x20000>;
		};
	};
};

&usbmisc {
	status = "okay";
};

&usbotg {
	over-current-active-high;
};

&usbhost1 {
	over-current-active-high;
};
