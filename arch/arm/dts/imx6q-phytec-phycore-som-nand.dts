/dts-v1/;

#include <arm/imx6q.dtsi>
#include "imx6q.dtsi"
#include "imx6qdl-phytec-phycore-som.dtsi"

/ {
	model = "Phytec phyCORE-i.MX6 Quad with NAND";
	compatible = "phytec,imx6q-pcm058-nand", "fsl,imx6q";

};

&ecspi1 {
	status = "okay";
};

&eeprom {
	status = "okay";
};

&fec {
	status = "okay";
	phy-handle = <&ethphy>;
	phy-mode = "rgmii";
	phy-reset-gpios = <&gpio1 14 1>;

	mdio {
		#address-cells = <1>;
		#size-cells = <0>;

		ethphy: ethernet-phy@3 {
			reg = <3>;
			max-speed = <1000>;
		};
	};
};

&flash {
	status = "okay";
};

&gpmi {
	status = "okay";
};

&usdhc1 {
	status = "okay";

	#address-cells = <1>;
	#size-cells = <1>;

	partition@0 {
		label = "barebox";
		reg = <0x0 0x80000>;
	};

	partition@1 {
		label = "barebox-environment";
		reg = <0x80000 0x80000>;
	};
};
